{% block moreScript %}
	<script type="text/javascript">
		$(document).ready(function() {
			$('#modal').on('show.bs.modal', function (e) {
		        let url = e.relatedTarget.href;
		        $.get(url, function (data) {
		            $('.modal-body').html(data);
		        });
		    })
		});
	</script>
{% endblock %}